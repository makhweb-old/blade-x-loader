{
    "name": "makhweb/blade-x-loader",
    "description": "Your Package Description here",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eldorbek Makhkamov",
            "email": "eldorbekmakhkamov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":true,
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Makhweb\\BladeXLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Makhweb\\BladeXLoader\\Tests\\":"tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-install-cmd": [
            "php artisan vendor:publish --tag=publishable --force"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=publishable --force"
        ],
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makhweb\\BladeXLoader\\BladeXLoaderServiceProvider"
            ],
            "aliases": {
                "Makhweb": "Makhweb\\BladeXLoader\\BladeXLoaderFacade"
            }
        }
    }
}